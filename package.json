{
	"name": "near-intents-cli",
	"version": "0.3.0",
	"description": "CLI for NEAR Intents - cross-chain token swaps, deposits, transfers, and withdrawals",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"bin": {
		"near-intents-cli": "./dist/cli.cjs"
	},
	"files": [
		"dist",
		"README.md"
	],
	"sideEffects": false,
	"keywords": [
		"near",
		"intents",
		"cross-chain",
		"swap",
		"defi",
		"defuse",
		"blockchain",
		"crypto",
		"ethereum",
		"solana"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/NEARBuilders/near-intents-cli"
	},
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"build": "tsdown",
		"prepublishOnly": "pnpm build",
		"start": "node dist/cli.mjs",
		"dev": "tsx watch src/cli.ts",
		"cli": "tsx src/cli.ts",
		"test": "vitest run",
		"test:watch": "vitest",
		"changeset": "changeset",
		"version": "changeset version",
		"ci:version": "changeset version && pnpm install --lockfile-only",
		"release": "pnpm build && changeset publish --access public",
		"lint": "biome check --write --unsafe"
	},
	"dependencies": {
		"@defuse-protocol/intents-sdk": "^0.49.0",
		"@defuse-protocol/internal-utils": "0.26.0",
		"@defuse-protocol/one-click-sdk-typescript": "^0.1.15",
		"@scure/base": "^2.0.0",
		"@thames/monads": "^0.7.0",
		"borsh": "^2.0.0",
		"borsher": "^4.0.0",
		"bs58": "^6.0.0",
		"dotenv": "^16.4.7",
		"fuse.js": "^7.1.0",
		"js-sha256": "^0.11.1",
		"near-api-js": "^6.1.0",
		"tweetnacl": "^1.0.3",
		"viem": "^2.37.8",
		"zod": "^4.0.14"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@changesets/cli": "^2.29.8",
		"@types/node": "^22.14.1",
		"near-kit": "^0.10.0",
		"tsdown": "^0.16.8",
		"tsx": "^4.7.1",
		"typescript": "^5.8.3",
		"vite-tsconfig-paths": "^6.0.4",
		"vitest": "^4.0.17"
	}
}
