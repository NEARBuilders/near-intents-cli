{
  "name": "near-intents-cli",
  "version": "0.1.0",
  "description": "SDK and CLI for NEAR Intents - cross-chain token swaps, deposits, and withdrawals",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "bin": {
    "near-intents": "./dist/cli.cjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "keywords": [
    "near",
    "intents",
    "cross-chain",
    "swap",
    "defi",
    "defuse",
    "blockchain",
    "crypto",
    "ethereum",
    "solana"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsdown src/index.ts src/cli.ts --format esm,cjs --dts --clean",
    "prepublishOnly": "pnpm build",
    "start": "node dist/cli.cjs",
    "dev": "tsx watch src/cli.ts",
    "cli": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish --access public"
  },
  "dependencies": {
    "@defuse-protocol/intents-sdk": "^0.46.1",
    "@defuse-protocol/internal-utils": "0.24.1",
    "@defuse-protocol/one-click-sdk-typescript": "^0.1.1-0.2",
    "@scure/base": "^2.0.0",
    "borsh": "^2.0.0",
    "borsher": "^4.0.0",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.7",
    "fuse.js": "^7.1.0",
    "js-sha256": "^0.11.1",
    "near-api-js": "^7.0.4",
    "tweetnacl": "^1.0.3",
    "viem": "^2.37.8",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^22.14.1",
    "tsdown": "^0.16.8",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  }
}
